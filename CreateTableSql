CREATE TABLE CUSTOMERS
(
  ID BIGINT PRIMARY KEY NOT NULL IDENTITY,
  FIRST_NAME VARCHAR(45),
  LAST_NAME VARCHAR(45),
  PATRONYMIC VARCHAR(45),
  TELEPHONE_NUMBER BIGINT
);
