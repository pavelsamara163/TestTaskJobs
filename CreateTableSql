CREATE TABLE ORDER_CUSTOMERS
(
  ID_ORDER BIGINT PRIMARY KEY NOT NULL IDENTITY,
  DESCRIPTION VARCHAR(255),
  CLIENT_ORDER VARCHAR(45),
  DATE_CREATION VARCHAR(45),
  DATE_COMPLETE VARCHAR(45),
  PRICE BIGINT,
  STATUS VARCHAR(45)
);
